{% extends 'base.html.twig' %}

{% block title %}Address{% endblock %}

{% block body %}
    <h1>Address</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ address.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ address.name }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ address.firstname }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ address.address }}</td>
            </tr>
            <tr>
                <th>Cp</th>
                <td>{{ address.cp }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ address.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ address.country }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_adminaddress_index') }}">back to list</a>

    <a href="{{ path('app_adminaddress_edit', {'id': address.id}) }}">edit</a>

    {{ include('admin/address/_delete_form.html.twig') }}
{% endblock %}
